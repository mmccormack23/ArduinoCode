//out is output, inp is input, com is common, alm is alarm, pls is pulse, rst is reset
const int pos_bphase_pls_out=30;
const int neg_bphase_pls_out=31;
const int pos_aphase_pls_out=32;
const int neg_aphase_pls_out=33;
const int out_com=34;
const int gnd=35;
const int move_=36;
const int almb_out=37;
const int pls_ready=38;
const int ready_out=39;
const int home_end=40;
const int ctrl_out_inp_pos=41;
const int fw_jog=42;
const int rv_jog=43;
const int in4_7_inp=44;
const int in8_9_inp_com=45;
const int stop_inp=46;
const int alm_rst=47;
const int zhome_inp=48;
const int free_inp=49;
const int ccw_dir_pos=50;
const int ccw_dir_neg=51;
const int cw_pls_pos=52;
const int cw_pls_neg=53;

//states of each int
int pos_bphase_pls_out_State=0;
int neg_bphase_pls_out_State=0;
int pos_aphase_pls_out_State=0;
int neg_aphase_pls_out_State=0;
int out_com_State=0;
int gnd_State=0;
int move_State=0;
int almb_out_State=0;
int pls_ready_State=0;
int ready_out_State=0;
int home_end_State=0;
int ctrl_out_inp_pos_State=0;
int fw_jog_State=0;
int rv_jog_State=0;
int in4_7_inp_State=0;
int in8_9_inp_com_State=0;
int stop_inp_State=0;
int alm_rst_State=0;
int zhome_inp_State=0;
int free_inp_State=0;
int ccw_dir_pos_State=0;
int ccw_dir_neg_State=0;
int cw_pls_pos_State=0;
int cw_pls_neg_State=0;

void setup() {
  // put your setup code here, to run once:
  pinMode(pos_bphase_pls_out, OUTPUT);
  pinMode(neg_bphase_pls_out, OUTPUT);
  pinMode(pos_aphase_pls_out, OUTPUT);
  pinMode(neg_aphase_pls_out, OUTPUT);
  pinMode(out_com, OUTPUT);
  pinMode(gnd, OUTPUT);
  pinMode(move_, OUTPUT);
  pinMode(almb_out, OUTPUT);
  pinMode(pls_ready, OUTPUT);
  pinMode(ready_out, OUTPUT);
  pinMode(home_end, OUTPUT);
  pinMode(ctrl_out_inp_pos, OUTPUT);
  pinMode(fw_jog, OUTPUT);
  pinMode(rv_jog, OUTPUT);
  pinMode(in4_7_inp, OUTPUT);
  pinMode(in8_9_inp_com, OUTPUT);
  pinMode(stop_inp, OUTPUT);
  pinMode(alm_rst, OUTPUT);
  pinMode(zhome_inp, OUTPUT);
  pinMode(free_inp, OUTPUT);
  pinMode(ccw_dir_pos, OUTPUT);
  pinMode(ccw_dir_neg, OUTPUT);
  pinMode(cw_pls_pos, OUTPUT);
  pinMode(cw_pls_neg, OUTPUT);
}
void loop() {
  // put your main code here, to run repeatedly: remember to end with }
